{
  "builds": [
    {
      "src": "src/pages/**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/scss/css/*.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/", "dest": "../../src/pages/index.html" },
    { "src": "/login", "dest": "../../src/pages/login.html" },
    { "src": "/signup", "dest": "../../src/pages/signup.html" },
    { "src": "/todo", "dest": "../../src/pages/todo.html" }
  ]
}
